    <script>
        function runDemo(type) {
            const output = document.getElementById('demo-output');
            output.classList.add('active');
            output.innerHTML = '<p style="color: #f59e0b; font-size: 1.3em;">‚è≥ AI processing your query...</p>';
            
            setTimeout(() => {
                let result = '';
                
                switch(type) {
                    case 'sourcing':
                        result = `
